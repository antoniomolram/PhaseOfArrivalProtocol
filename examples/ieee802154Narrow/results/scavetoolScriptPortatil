scavetool scalar -p "module(**computer**) AND name(**Number**) AND name(**packets**) AND name(**sent**) AND name(**anchor20**)" -O ./ScavetoolPortatil/SendFromAN-20.csv -F csv *.sca -V

scavetool scalar -p "module(**anchor[*]**) AND name(**Number**) AND name(**from*this*anchor**)" -O ./ScavetoolPortatil/pktFromThisAnchor.csv -F csv *.sca -V


scavetool scalar -p "module(**anchor[8]**) AND name(**Number**) AND name(**ack-ComSink1**)" -O ./ScavetoolPortatil/NoAck-8.csv -F csv *.sca -V
scavetool scalar -p "module(**anchor[8]**) AND name(**Number**) AND name(**CAF**) AND name(**ComSink1**)" -O ./ScavetoolPortatil/caf-8.csv -F csv *.sca -V
scavetool scalar -p "module(**anchor[8]**) AND name(**Number**) AND name(**packets**) AND name(**transmitted**)" -O ./ScavetoolPortatil/tx-8.csv -F csv *.sca -V
scavetool scalar -p "module(**anchor[8]**) AND name(**packets**) AND name(**App**) AND name(**Queue**) AND name(**ComSink1**)" -O ./ScavetoolPortatil/appPkt-8.csv -F csv *.sca -V
scavetool scalar -p "module(**anchor[8]**) AND name(**Number**) AND name(**AN**) AND name(**Reports**) AND name(**ACK**)" -O ./ScavetoolPortatil/repAck-8.csv -F csv *.sca -V 
scavetool scalar -p "NOT(**really**) AND module(**anchor[8]**) AND (Number*of*Reports*received*in*AN*)" -O ./ScavetoolPortatil/received-8.csv -F csv *.sca -V
scavetool scalar -p "module(**anchor[8]**) AND name(**Number**) AND name(**from*this*anchor**)" -O ./ScavetoolPortatil/pktFromThisAnchor-8.csv -F csv *.sca -V
scavetool scalar -p "module(**anchor[8]**) AND name(**Effectiveness**) AND (**each**)" -O ./ScavetoolPortatil/Effectiveness-8.csv -F csv *.sca -V


