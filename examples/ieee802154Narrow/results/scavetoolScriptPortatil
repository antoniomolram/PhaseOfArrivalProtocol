

scavetool scalar -p "module(**computer**) AND name(**Number**) AND name(**received**) AND name(**really**)" -O ./ScavetoolPortatil/out.csv -F csv *.sca -V
scavetool scalar -p "module(**computer**) AND name(**Number**) AND name(**no*duplicated**) AND name(**reports**)" -O ./ScavetoolPortatil/reportsNoDup.csv -F csv *.sca -V

scavetool scalar -p "module(**computer**) AND name(**Number**) AND name(**packets**) AND name(**sent**) AND name(**mobile**) AND name(**node*0**)" -O ./ScavetoolPortatil/SendFromMN.csv -F csv *.sca -V

scavetool scalar -p "module(**anchor**) AND name(**Number**) AND name(**packets**) AND name(**sent**) AND name(**mobile**) AND name(**node*0**)" -O ./ScavetoolPortatil/SendFromMN-AN.csv -F csv *.sca -V

scavetool scalar -p "module(**computer**) AND name(**Number**) AND name(**packets**) AND name(**sent**) AND name(**anchor8**)" -O ./ScavetoolPortatil/SendFromAN-8.csv -F csv *.sca -V

scavetool scalar -p "module(**computer**) AND name(**Number**) AND name(**packets**) AND name(**sent**) AND name(**anchor12**)" -O ./ScavetoolPortatil/SendFromAN-12.csv -F csv *.sca -V

