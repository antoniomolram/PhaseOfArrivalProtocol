

scavetool scalar -p "module(**computer**) AND name(**Number**) AND name(**received**) AND name(**really**)" -O ./ScavetoolPortatil/out.csv -F csv *.sca -V
scavetool scalar -p "module(**computer**) AND name(**Number**) AND name(**no*duplicated**) AND name(**reports**)" -O ./ScavetoolPortatil/reportsNoDup.csv -F csv *.sca -V

scavetool scalar -p "module(**computer**) AND name(**Number**) AND name(**packets**) AND name(**sent**) AND name(**mobile**) AND name(**node*0**)" -O ./ScavetoolPortatil/SendFromMN.csv -F csv *.sca -V

scavetool scalar -p "module(**anchor[*]**) AND name(**Number**) AND name(**from*this*anchor**)" -O ./ScavetoolPortatil/pktFromThisAnchor.csv -F csv *.sca -V


scavetool scalar -p "module(**anchor[4]**) AND name(**Number**) AND name(**ack-ComSink1**)" -O ./ScavetoolPortatil/NoAck-4.csv -F csv *.sca -V
scavetool scalar -p "module(**anchor[4]**) AND name(**Number**) AND name(**CAF**) AND name(**ComSink1**)" -O ./ScavetoolPortatil/caf-4.csv -F csv *.sca -V
scavetool scalar -p "module(**anchor[4]**) AND name(**Number**) AND name(**packets**) AND name(**transmitted**)" -O ./ScavetoolPortatil/tx-4.csv -F csv *.sca -V
scavetool scalar -p "module(**anchor[4]**) AND name(**packets**) AND name(**App**) AND name(**Queue**) AND name(**ComSink1**)" -O ./ScavetoolPortatil/appPkt-4.csv -F csv *.sca -V
scavetool scalar -p "module(**anchor[4]**) AND name(**Number**) AND name(**AN**) AND name(**Reports**) AND name(**ACK**)" -O ./ScavetoolPortatil/repAck-4.csv -F csv *.sca -V 
scavetool scalar -p "NOT(**really**) AND module(**anchor[4]**) AND (Number*of*Reports*received*in*AN*)" -O ./ScavetoolPortatil/received-4.csv -F csv *.sca -V


